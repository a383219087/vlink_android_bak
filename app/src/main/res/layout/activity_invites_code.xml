<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools">

    <data>

        <variable
            name="_all"
            type="com.yjkj.chainup.new_version.activity.personalCenter.vm.MyInviteCodesViewModel" />

        <import type="android.view.View" />

        <import type="com.yjkj.chainup.R" />

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#F5F5F5">

        <com.yjkj.chainup.new_version.view.PersonalCenterView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:showRightTv="true"
            app:rightTitle="添加"
            app:personalTitle="我的邀请码" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_about"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:itemBinding="@{_all.itemBinding}"
            app:items="@{_all.items}"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            />


    </LinearLayout>


</layout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_50"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/tv_title"
        style="@style/textStyle_title_16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_weight="0.4"
        android:text="线路1"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/tv_content_header"
        style="@style/textStyle_value_82_12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_weight="0.28"
        android:button="@null"
        android:text="@string/customSetting_title_apiSpeed"
        android:textColor="@color/normal_text_color"
        android:textSize="@dimen/sp_16" />

    <TextView
        android:id="@+id/tv_content_ws_header"
        style="@style/textStyle_value_82_12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_weight="0.32"
        android:button="@null"
        android:text="@string/customSetting_title_wsSpeed"
        android:textColor="@color/normal_text_color"
        android:textSize="@dimen/sp_16" />


</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <!--    android:background="@drawable/bg_transaction_item"
    -->
    <LinearLayout
        android:id="@+id/ll_item_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/transparent"
        android:orientation="horizontal"
        android:paddingTop="@dimen/dp_5"
        android:paddingBottom="@dimen/dp_5">

        <TextView
            android:id="@+id/tv_quantity_item_for_depth_buy"
            style="@style/textStyle_text_nor_12"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical|left"
            android:lines="1"
            android:text="--"
            tools:text="4386.33000000000000000" />


        <TextView
            android:id="@+id/tv_price_item_for_depth_buy"
            style="@style/textStyle_text_nor_12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_vertical|left"
            android:lines="1"
            android:text="--"
            tools:text="4386000000.330000000000000" />

        <TextView
            android:id="@+id/tv_price_item_for_buy_range"
            style="@style/textStyle_text_nor_12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_8"
            android:layout_weight="1"
            android:gravity="center_vertical|right"
            android:lines="1"
            android:text="--"
            tools:text="43860" />


    </LinearLayout>
</FrameLayout>
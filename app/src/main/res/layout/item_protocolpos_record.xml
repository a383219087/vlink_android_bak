<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/dp_15"
    android:paddingTop="@dimen/dp_20"
    android:paddingRight="@dimen/dp_15"
    android:paddingBottom="@dimen/dp_20"
    >

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/text_color"
        android:textSize="@dimen/sp_16"
        android:textStyle="bold"
        tools:text="ZEL"
        />

    <TextView
        android:id="@+id/tv_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:textColor="@color/normal_text_color"
        android:textSize="@dimen/sp_12"
        tools:text="锁仓中"
        />

    <TextView
        android:id="@+id/tv_startTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_name"
        android:layout_marginTop="@dimen/dp_16"
        android:text="@string/pos_string_lockBeginTime"
        android:textColor="@color/normal_text_color"
        android:textSize="@dimen/sp_12"
        />

    <TextView
        android:id="@+id/tv_stime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_status"
        android:layout_alignParentRight="true"
        android:layout_marginTop="@dimen/dp_16"
        android:textColor="@color/text_color"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        tools:text="2016/01/16 15:00:23"
        />

    <TextView
        android:id="@+id/tv_lockNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_startTime"
        android:layout_marginTop="@dimen/dp_12"
        android:text="@string/pos_string_lockNumber"
        android:textColor="@color/normal_text_color"
        android:textSize="@dimen/sp_12"
        />

    <TextView
        android:id="@+id/tv_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_stime"
        android:layout_alignParentRight="true"
        android:layout_marginTop="@dimen/dp_10"
        android:textColor="@color/text_color"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        tools:text="10000"
        />

    <TextView
        android:id="@+id/tv_annualized_income"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_lockNumber"
        android:layout_marginTop="@dimen/dp_12"
        android:text="@string/pos_string_interestRate"
        android:textColor="@color/normal_text_color"
        android:textSize="@dimen/sp_12"
        />

    <TextView
        android:id="@+id/tv_income"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_number"
        android:layout_alignParentRight="true"
        android:layout_marginTop="@dimen/dp_10"
        android:textColor="@color/text_color"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        tools:text="8.88%"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_annualized_income"
        android:layout_marginTop="@dimen/dp_12"
        android:text="@string/pos_string_currentEran"
        android:textColor="@color/normal_text_color"
        android:textSize="@dimen/sp_12"
        />

    <TextView
        android:id="@+id/tv_current_income"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_income"
        android:layout_alignParentRight="true"
        android:layout_marginTop="@dimen/dp_10"
        android:drawableRight="@drawable/enter"
        android:drawablePadding="@dimen/dp_5"
        android:textColor="@color/text_color"
        android:textSize="@dimen/sp_14"
        android:textStyle="bold"
        tools:text="5000"
        />


</RelativeLayout>